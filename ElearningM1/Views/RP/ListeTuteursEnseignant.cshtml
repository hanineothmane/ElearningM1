
@{
    ViewBag.Title = "Liste des tuteurs";
}
<head>
    <link rel="stylesheet" media="screen" type="text/css" title="Exemple" href="~/Content/css/materialize.css" />
    <link rel="stylesheet" media="screen" type="text/css" title="Exemple" href="~/Content/css/materialize.min.css" />
    <link rel="stylesheet" media="screen" type="text/css" title="Exemple" href="~/Content/css/style.css" />
</head>
<h2>Les tuteurs enseignant</h2>
<body>
    <table class="striped">
        <thead>
            <tr>
                <th>Nom</th>
                <th>Prénom</th>
                <th>Date de naissance</th>
                <th>Courriel</th>
                <th>Téléphone</th>
            </tr>
        </thead>
        @foreach (ElearningM1.Models.TuteurEnseignant te in ViewData["TuteursEnseignant"] as List<ElearningM1.Models.TuteurEnseignant>)
        {
            <tr>
                <td>@Html.ActionLink(te.Nom, "#", "#", new { nom = te.Nom }, null)</td>
                <td>@Html.ActionLink(te.Prenom, "#", "#", new { nom = te.Nom }, null)</td>
                <td>@te.DateNaiss</td>
                <td>@te.Courriel</td>
                <td>@te.Adresse</td>
            </tr>
        }
    </table>
</body>
