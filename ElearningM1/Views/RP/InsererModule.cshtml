@model ElearningM1.Models.Module
@{
    ViewBag.Title = "InsererModule";
}

<h2>InsererModule</h2>
    @using (Html.BeginForm())
    {

        <div class="form-group">
            @Html.LabelFor(m => m.Nom)
            @Html.TextBoxFor(m => m.Nom, new { @class = "form-control" })
        </div>

        <div class="form-group">
            @Html.LabelFor(m => m.Coeff)
            @Html.TextBoxFor(m => m.Coeff, new { @class = "form-control" })
        </div>

        <div class="form-group">
            @Html.LabelFor(m => m.EstNational)
            @Html.TextBoxFor(m => m.EstNational, new { @class = "form-control" })
        </div>

    <input type="submit" class="btn btn-primary" value="Valider" />
    }
