@model ElearningM1.Models.A_TE_Module_View
<html>
<head>
    @{
        ViewBag.Title = "Affecter_A_Te";
    }
</head>
<body>
    <h2>Affecter_A_Te</h2>



    <h2>veuillez selectionner : </h2>

    <span style="color:red">@ViewBag.erreur</span>

    <form method="post" action="~/TuteurEnseignant/insert_A_TE" >
        <fieldset>
            <legend>Affectation</legend>
            <div>
                <label>Un Apprenant : </label>
            </div>

            <div class="form-group">
                <select id="apprenant" name="apprenant">

                    @{
                        foreach (var apprenant in Model.Apprenant)
                        {
                            <option value=@apprenant.Id> @apprenant.Nom</option>

                        }
                    }
                </select>

            </div>
            <div>
                <label> Un Tuteur Enseignant : </label>
            </div>

            <div class="form-group">
                <select id="te" name="te">

                    @{
                        foreach (var te in Model.Te)
                        {
                            <option value=@te.Id> @te.Nom</option>

                        }
                    }

                </select>
            </div>
            

            <div class="form-group">
                <label>&nbsp;</label>
                <input type="submit" value="Affecter" />
            </div>

        </fieldset>
        </form>
                      
                       


</body>
</html>