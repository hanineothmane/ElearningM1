@model IEnumerable<ElearningM1.Models.TuteurEnseignant>
<html>
<head>
    @{
        ViewBag.Title = "AllTuteurEnseigant";
    }
    <link rel="stylesheet" media="screen" type="text/css" title="Exemple" href="~/Content/css/materialize.css" />
    <link rel="stylesheet" media="screen" type="text/css" title="Exemple" href="~/Content/css/materialize.min.css  />
    <link rel="stylesheet" media="screen" type="text/css" title="Exemple" href="~/Content/css/style.css" />
</head>
<body>
    <h2>Veuillez selectionner un apprenant </h2>



    <table class="striped">
        <thead>
            <tr>
                <th>ID</th>
                <th>Nom du Tuteur</th>
                <th>Prenom du Tuteur</th>
                <th>Date de naisse du Tuteur</th>
                <th>Courriel du Tuteur</th>
                <th>Telephone du Tuteur</th>
            </tr>
        </thead>

        <tbody>
            @{ foreach (var tuteur in Model)
            {
                    <tr>
                        <td>@Html.ActionLink(tuteur.Nom, "InfoTe" , "TuteurEnseignant", new { id = tuteur.Id } , null) </td>
                        <td>@Html.ActionLink(tuteur.Prenom, "InfoTe", "TuteurEnseignant", new { id = tuteur.Id }, null) </td>
                        
                    </tr>
                }
            }
        </tbody>

    </table>
</body>

</html>